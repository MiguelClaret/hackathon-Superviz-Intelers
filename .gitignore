# Ignore bundler config.
/.bundle

# Ignore the SQLite database, if used.
*.sqlite3
*.sqlite3-journal

# Ignore the database environment variables file.
.env
.env.*

# Ignore logs and temp files.
/railsapp/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore all files in the `storage` directory except `.keep` files.
/storage/*
!/storage/.keep

# Ignore uploaded files in development.
/public/system/*
/public/uploads

# Ignore pid files and other server-related files.
/tmp/pids/*
/tmp/cache/*
/tmp/sockets/*
/tmp/webpacker/*

# Ignore yarn integrity and cache files.
/node_modules
/yarn-error.log
/yarn-debug.log*

# Ignore Sprockets and Rails asset pipeline cache.
/public/assets
/public/packs
/public/packs-test
/public/packs-dev
/public/packs-staging
/public/packs-production
/public/packs-manifest.json
/public/assets-manifest.json

# Ignore compiled files.
/vendor/assets/bower_components/
/vendor/assets/node_modules/
node_modules/
node_modules/*

# Ignore npm and Yarn files.
npm-debug.log
yarn-debug.log
yarn-error.log

# Ignore coverage reports generated by tools like SimpleCov.
/coverage/

# Ignore sensitive credential files.
/config/master.key
/config/credentials.yml.enc

# Ignore other sensitive files.
/config/database.yml
/config/secrets.yml

# Ignore byebug command history file.
/.byebug_history

# Ignore JetBrains IDE files (RubyMine, IntelliJ IDEA).
.idea/

# Ignore MacOS file.
/.DS_Store

# Ignore Vagrant files.
/.vagrant/

# Ignore temporary files.
/*.rbc
/.sass-cache/
/.rspec
/.rspec-status

# Ignore dotenv files.
/.env
.env.*

# Ignore Spring files.
/spring/*.pid

# Ignore bundler's cache.
vendor/bundle
.bundle
/vendor/bundle

# Ignore AWS CLI cache.
/.aws/

# Ignore the result of "rake notes" with "rake notes:optimize", "rake notes:fixme",
and "rake notes:todo".
/db/schema.rb
/db/structure.sql

# Ignore files created by editors or operating systems.
/*.swp
/*~
/.project
/.idea
/.vscode

# Ignore specific files in railsapp and exemplosSuperViz directories
/railsapp/*
!/railsapp/Dockerfile
!/railsapp/docker-compose.yml

/exemplosSuperViz/*
